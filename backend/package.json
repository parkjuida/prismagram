{
  "name": "prismagram",
  "version": "1.0.0",
  "engines": {
    "node": "12"
  },
  "description": "Instagram clone with Express + prisma + react + nodejs",
  "main": "index.js",
  "repository": "https://github.com/parkjuida/prismagram.git",
  "author": "parkjuida",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "@babel/runtime": "^7.6.3",
    "aws-sdk": "^2.548.0",
    "copy": "^0.3.2",
    "dotenv": "^7.0.0",
    "graphql-tools": "^4.0.4",
    "graphql-yoga": "^1.17.4",
    "heroku-cli": "^7.0.9",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.5.8",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.0.0",
    "nodemailer-sendgrid": "^1.0.3",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "nodemon": "^1.18.10",
    "npx": "^10.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prisma-client-lib": "^1.29.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.6.2",
    "nodemon": "^1.18.10"
  },
  "scripts": {
    "deploy": "prisma deploy",
    "generate": "prisma generate",
    "prisma": "yarn run deploy && yarn run generate",
    "dev": "nodemon --exec babel-node src/server.js",
    "prebuild": "yarn run prisma",
    "build": "babel src -d build",
    "postbuild": "cd src && npx copy api/**/*.graphql ../build/api",
    "start": "node build/server.js"
  }
}
